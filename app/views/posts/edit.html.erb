<h1>Edit Places</h1>
<%= form_for @post do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :latitude %>
	<%= f.text_field :latitude %>
	<br>
	<%= f.label :longitude %>
	<%= f.text_field :longitude %>
	<br>
	<%= f.submit 'Update', class: 'button radius small' %>
	<% end %>